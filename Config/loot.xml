<configs>
  <!--60 Slots - BEGIN-->
  <!-- Make player dropped bags bigger to accomodate new size-->
  <set xpath="/lootcontainers/lootcontainer[@id='2']/@size">8,10</set>
  <!--60 Slots - END-->

  <!--UH-60 Helicopter - BEGIN-->
  <insertBefore xpath="/lootcontainers/lootgroup[@name='groupQuestChallenge']">
    <lootgroup name="UH60group">
      <item name="vehicleUH60chassis"/>
      <item name="vehicleUH60accessories"/>
    </lootgroup>
  </insertBefore>

  <append xpath="/lootcontainers/lootgroup[@name='vehicleParts']">
    <item group="UH60group" loot_prob_template="probTemplate0.35"/>
  </append>
  <!--UH-60 Helicopter - END-->

  <!--Intruders Vehicles - BEGIN-->
  <!--Container Size for all vehicles-->
  <insertAfter xpath="/lootcontainers/lootcontainer[@id='124']">
    <lootcontainer id="222" count="0" size="10,9" sound_open="UseActions/open_shopping_basket" open_time="0" sound_close="UseActions/close_shopping_basket" loot_quality_template="qualBaseTemplate">
    </lootcontainer>
  </insertAfter>
  <!--Intruders Vehicles - END-->

  <!--Intruders Vehicles - BEGIN-->
  <!--Schematic Loot Group-->
  <append xpath="/lootcontainers/lootgroup[@name='schematicsVehiclesRare']">
    <item name="vehicleATeamVanChassisSchematic"/>
    <item name="vehicleATeamVanAccessoriesSchematic"/>
    <item name="vehicleMowerChassisSchematic"/>
    <item name="vehicleMowerAccessoriesSchematic"/>
    <item name="vehicleKolunChassisSchematic"/>
    <item name="vehicleKolunAccessoriesSchematic"/>
    <item name="vehicleAxelWheelSchematic"/>
    <item name="vehicleAxelAxleSchematic"/>
    <item name="vehicleAlphaWheelSchematic"/>
    <item name="vehicleAlphaChassisSchematic"/>
    <item name="vehicleAlphaAccessoriesSchematic"/>
    <item name="vehicleTumblerChassisSchematic"/>
    <item name="vehicleTumblerAccessoriesSchematic"/>
    <item name="vehicleHearseChassisSchematic"/>
    <item name="vehicleHearseAccessoriesSchematic"/>
    <item name="vehicleRoadsterChassisSchematic"/>
    <item name="vehicleRoadsterAccessoriesSchematic"/>
  </append>
  <!--Intruders Vehicles - END-->
</configs>