<configs>
	<!--60 Slots - BEGIN-->
	<!-- Edit player entity to increase inventory size -->
	<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='CarryCapacity'][@value='27']/@value">40</set>
